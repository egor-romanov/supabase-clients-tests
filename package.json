    "scripts": {
        "lint": "eslint framework/**/*.ts test/**/*.ts js-adapter/**/*.ts",
        "clean:report": "rm -rf allure-report",
        "clean:results": "rm -rf allure-results",
        "clean:all": "concurrently \"yarn clean:report\" \"yarn clean:results\"",
